<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fragment_adventure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_hammer"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_hammer"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".login.Activity_Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".user.Activity_User"
            android:label="@string/app_name">
        </activity>

        <activity android:name=".owner.Activity_Owner"
            android:label="@string/app_name">
        </activity>

    </application>

</manifest>